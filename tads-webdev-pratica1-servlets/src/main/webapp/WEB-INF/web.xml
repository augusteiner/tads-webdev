<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
  version="3.0">

  <display-name>1ª Avaliação Prática (Servlets)</display-name>

  <servlet>
    <servlet-name>Questao_01</servlet-name>
    <servlet-class>tads.web.pratica.servlets.q1.Questao_01_HttpServlet</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>Questao_02</servlet-name>
    <servlet-class>tads.web.pratica.servlets.q2.Questao_02_HttpServlet_1</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>Questao_02_2</servlet-name>
    <servlet-class>tads.web.pratica.servlets.q2.Questao_02_HttpServlet_2</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>Questao_03</servlet-name>
    <servlet-class>tads.web.pratica.servlets.q3.Questao_03_HttpServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>Questao_01</servlet-name>
    <url-pattern>/questao_01/*</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>Questao_02</servlet-name>
    <url-pattern>/questao_02/*</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>Questao_02_2</servlet-name>
    <url-pattern>/questao_02_2/*</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>Questao_03</servlet-name>
    <url-pattern>/questao_03/*</url-pattern>
  </servlet-mapping>

</web-app>
